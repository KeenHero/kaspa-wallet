{
  "name": "@keenherox/kaspa-wallet",
  "version": "1.0.0",
  "description": "Kaspa desktop wallet",
  "author": "KeenHero",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KeenHero/kaspa-wallet.git"
  },
  "homepage": "https://github.com/KeenHero/kaspa-wallet#readme",
  "bugs": {
    "url": "https://github.com/KeenHero/kaspa-wallet/issues"
  },
  "keywords": [
    "kaspa",
    "wallet",
    "electron",
    "desktop",
    "crypto"
  ],
  "main": "electron/main.cjs",
  "bin": {
    "kaspa-wallet": "bin/kaspa-wallet.cjs"
  },
  "files": [
    "dist/**/*",
    "electron/**/*",
    "bin/**/*",
    "README.md",
    "screenshot.jpg",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=20"
  },
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:renderer": "vite --port 5173 --strictPort",
    "dev:electron": "wait-on tcp:5173 && cross-env VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "dev:desktop": "concurrently -k \"npm:dev:renderer\" \"npm:dev:electron\"",
    "build": "tsc -b && vite build",
    "prepack": "npm run build",
    "build:desktop": "npm run build && electron-builder --dir",
    "dist:desktop": "npm run build && electron-builder",
    "start:desktop": "electron .",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "build": {
    "appId": "com.kaspa.wallet",
    "productName": "Kaspa Wallet",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "extraMetadata": {
      "main": "electron/main.cjs"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "artifactName": "${productName}-Setup-${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "dependencies": {
    "@noble/curves": "^1.9.7",
    "@noble/hashes": "^1.4.0",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-tooltip": "^1.1.2",
    "bip39": "^3.1.0",
    "bitcoinjs-lib": "^6.1.5",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "ecpair": "^2.1.0",
    "electron": "^37.2.6",
    "framer-motion": "^11.3.24",
    "lucide-react": "^0.436.0",
    "qrcode.react": "^3.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.0",
    "tailwind-merge": "^2.4.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/bip39": "^3.0.0",
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "electron-builder": "^25.1.8",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "postcss": "^8.4.41",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.1.0",
    "vite": "^5.4.1",
    "wait-on": "^8.0.1"
  }
}
